{
  "name": "contributte/doctrine-mongodb",
  "description": "Doctrine MongoDB for Nette Framework",
  "keywords": ["nette", "contributte", "doctrine", "mongodb", "database", "document"],
  "type": "library",
  "license": "MIT",
  "homepage": "https://github.com/contributte/doctrine-mongodb",
  "authors": [
    {
      "name": "Milan Felix Sulc",
      "homepage": "https://f3l1x.io"
    },
    {
      "name": "David Solc",
      "homepage": "https://solc.dev"
    }
  ],
  "require": {
    "php": ">=8.2",
    "ext-mongodb": "*",
    "mongodb/mongodb": "^1.15.0",
    "nette/di": "^3.1.0",
    "nette/schema": "^1.2.0"
  },
  "require-dev": {
    "contributte/phpstan": "^0.1.0",
    "contributte/qa": "^0.4.0",
    "contributte/tester": "^0.3.0",
    "mockery/mockery": "^1.6.0"
  },
  "autoload": {
    "psr-4": {
      "Contributte\\DoctrineMongodb\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.3.x-dev"
    }
  }
}
